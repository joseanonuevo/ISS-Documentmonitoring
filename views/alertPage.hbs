<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Alert</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">

    <!-- Fonts -->
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300&display=swap" rel="stylesheet">    
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@900&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/css/styles.css" />
    <link rel="preconnect" href="https://fonts.gstatic.com">

    <!-- Icons -->
    <link href="https://unpkg.com/material-components-web@latest/dist/material-components-web.min.css" rel="stylesheet">
    <script src="https://unpkg.com/material-components-web@latest/dist/material-components-web.min.js"></script>
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    {{!-- Favicon --}}
    <link rel="icon" href="/img/iss-logo.png" />
</head>

<body style="background-color:#f8f7f6">

    <div class="design-top" style="width: 100%; height: 30px; background-color: orange;">

    </div>

    <div class="header-reset">
        <img src="/img/iss-logo.png" id="header-logo1">
        <p style="font-size:30px;">INFORMATION SYSTEMS SOCIETY</p>
    </div>

    <div class="resetpage-content" style="width: 50%;">
        <br><br>
        <center>
            {{#if noAccount}}
            <span class="material-icons" style="font-size: 250px; color:red">
                {{error}}
            </span>

            <h2 class="thick">
                {{noAccount}}
            </h2>
            {{/if}}

            {{#if yesAccount}}
            <span class="material-icons" style="font-size: 250px; color:rgb(23, 207, 23)">
                {{success}}
            </span>

            <h2 class="thick">
                {{yesAccount}}
            </h2>
            {{/if}}

            {{#if expiredLink}}
            <span class="material-icons" style="font-size: 250px; color:red">
                {{error}}
            </span>

            <h2 class="thick">
                {{expiredLink}}
            </h2>
            {{/if}}

            {{#if errorAlert}}
            <span class="material-icons" style="font-size: 250px; color:red">
                {{error}}
            </span>

            <h2 class="thick">
                {{errorAlert}}
            </h2>
            {{/if}}

            {{#if pwUpdated}}
            <span class="material-icons" style="font-size: 250px; color:rgb(23, 207, 23)">
                {{success}}
            </span>

            <h2 class="thick">
                {{pwUpdated}}
            </h2>
            {{/if}}

            {{#if successChangePw}}
            <span class="material-icons" style="font-size: 250px; color:rgb(9, 139, 9)">
                {{success}}
            </span>

            <h2 class="thick">
                {{successChangePw}}
            </h2>
            {{/if}}

            {{#if wrongOldPw}}
            <span class="material-icons" style="font-size: 250px; color:red">
                {{error}}
            </span>

            <h2 class="thick">
                {{wrongOldPw}}
            </h2>
            {{/if}}

            {{#if userExistsAlready}}
            <span class="material-icons" style="font-size: 250px; color:red">
                {{error}}
            </span>

            <h2 class="thick">
                {{userExistsAlready}}
            </h2>
            {{/if}}

            {{#if successActivateAccount}}
            <span class="material-icons" style="font-size: 250px; color:rgb(9, 139, 9)">
                {{success}}
            </span>

            <h2 class="thick">
                {{successActivateAccount}}
            </h2>
            {{/if}}

            {{#if incorrectOrExpiredLink1}}
            <span class="material-icons" style="font-size: 250px; color:red">
                {{error}}
            </span>

            <h2 class="thick">
                {{incorrectOrExpiredLink1}}
            </h2>
            {{/if}}

            {{#if invalidRequest}}
            <span class="material-icons" style="font-size: 250px; color:red">
                {{error}}
            </span>

            <h2 class="thick">
                {{invalidRequest}}
            </h2>
            {{/if}}

            {{#if errorMSG}}
            <span class="material-icons" style="font-size: 250px; color:red">
                {{error}}
            </span>

            <h2 class="thick">
                {{errorMSG}}
            </h2>
            {{/if}}

            {{#if successAccountRegistered}}
            <span class="material-icons" style="font-size: 250px; color:rgb(9, 139, 9)">
                {{success}}
            </span>

            <h2 class="thick">
                {{successAccountRegistered}}
            </h2>
            {{/if}}

            {{#if successEdit}}
            <span class="material-icons" style="font-size: 250px; color:rgb(9, 139, 9)">
                {{success}}
            </span>

            <h1 class="thick" >
                {{successEdit}}
            </h1>
            {{/if}}



            <button id="alertBtn" onclick="goBack()">Go Back</button>
        </center>
        <br>
    </div>
    <br>
    <br>
    <br>
    <br>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW" crossorigin="anonymous">
        </script>
    <script>
        function goBack() {
            window.history.back();
        }
    </script>
</body>


</html>